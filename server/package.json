{
  "name": "supermojo-interview",
  "version": "1.0.0",
  "private": true,
  "main": "build/index.js",
  "author": "alan-supermojo <alan@supermojo.com>",
  "license": "UNLICENSED",
  "scripts": {
    "dev": "NODE_ENV=dev nodemon src/index.ts",
    "db-client-gen": "prisma generate",
    "db-reset": "prisma migrate reset",
    "db-migrate": "prisma migrate deploy",
    "db-migrate-dev": "prisma migrate dev",
    "build": "rimraf build && yarn db-client-gen && tsc && cp -R ./prisma build/",
    "build-watch": "rimraf build && concurrently \"tsc --watch --noEmit --pretty\" \"nodemon -e prisma --watch ./prisma -x yarn db-client-gen\"",
    "docker": "docker compose up -d",
    "docker-stop": "docker compose down",
    "docker-kill": "docker compose down -v",
    "redis-cli": "docker compose exec -it redis ash -c 'redis-cli'",
    "pg-cli": "docker compose exec -it postgres bash -c 'psql -d $POSTGRES_DB -U $POSTGRES_USER'",
    "test": "NODE_ENV=test LOG_LEVEL=fatal jest"
  },
  "devDependencies": {
    "@tsconfig/node16": "^1.0.3",
    "@types/express": "^4.17.13",
    "@types/jest": "^29.2.5",
    "@types/node": "16",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.48.0",
    "concurrently": "^7.6.0",
    "eslint": "^8.23.0",
    "eslint-config-standard-with-typescript": "^24.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.5",
    "eslint-plugin-promise": "^6.0.1",
    "jest": "^29.3.1",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.1",
    "prettier-config-standard": "^5.0.0",
    "prisma": "^4.8.0",
    "rimraf": "^3.0.2",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.3"
  },
  "dependencies": {
    "@prisma/client": "^4.8.0",
    "@types/cors": "^2.8.13",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.1",
    "express-promise-router": "^4.1.1",
    "ioredis": "^5.2.4",
    "pino": "^8.5.0",
    "pino-http": "^8.2.0",
    "pino-pretty": "^9.1.0",
    "uuid": "^9.0.0",
    "zod": "^3.20.2"
  }
}
